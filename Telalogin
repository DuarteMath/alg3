    private void jbotloginActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbotloginActionPerformed
            if (jnome.getText().equals("")&&jcpf.getText().equals("")) {
                
                JOptionPane.showMessageDialog(null, "Bem Vindo");
                
            } else{      
                
                
            
                     JOptionPane.showMessageDialog(null, "falha no cadastro");
                     
        
            }
        
    }//GEN-LAST:event_jbotloginActionPerformed

